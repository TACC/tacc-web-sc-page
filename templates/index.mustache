<!--open styles-->
{{> _styles}}
<!--close styles-->
	<div id="sc-wrap" class="t-2019">
		<!-- WARNING: `#sc` and `.container` __must not__ be together, because `#sc` sets wide background color (without reliance on variable containers) and `.container` restricts content width. -->
		<div class="container">
			{{#booth}}
			<section class="sc-content sc-content--primary" id="sc-booth-events">
				<h1 class="sc-content__title">{{booth}}</h1>
				<div class="row">
					<!--open booth-events-->
					{{> _events.booth}}
					<!--close booth-events-->
					{{{booth_extra_markup}}}
				</div>
			</section>
			{{/booth}}
			{{^booth}}
			<!-- No primary section, this year, because no booth -->
			<!-- IDEA: Make the `--secondary` sections `--primary` (requires grander header) -->
			<!--<section class="sc-content sc-content--primary" id="sc-booth-events">â€¦</section>-->
			{{/booth}}
			<div class="row">
				<div class="col-md-8">
					{{#conf}}
					<section class="sc-content sc-content--secondary" id="sc-conf-events">
						<h1 class="sc-content__title">{{conf}}</h1>
						<!--open conf-events-->
						{{> _events.conf}}
						<!--close conf-events-->
						{{{conf_extra_markup}}}
					</section>
					{{/conf}}
				</div>
				<div class="col-md-4">
					<section class="sc-content sc-content--secondary" id="sc-social_media">
						<!--open social-media-->
						{{> _social-media}}
						<!--close social-media-->
						{{{social_extra_markup}}}
					</section>
				</div>
			</div>
		</div>
	</div>
