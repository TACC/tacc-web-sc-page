<!--open styles-->
{{> styles}}
<!--close styles-->
	<div id="sc-content">
		<div class="container">
			<section class="sc-content--primary" id="sc-booth-events">
				<h1 class="sc-content__title">SC19 TACC Booth Schedule</h1>
				<div class="row">
					<!--open booth-events-->
					{{> _events.booth}}
					<!--close booth-events-->
				</div>
			</section>
			<div class="row">
				<div class="col-md-8">
					<section class="sc-content--secondary" id="sc-conf-events">
						<h1 class="sc-content__title">SC19 TACC Participation</h1>
						<!--open conf-events-->
						{{> _events.conf}}
						<!--close conf-events-->
					</section>
				</div>
				<div class="col-md-4">
					<section class="sc-content--secondary" id="sc-social_media">
						<!--open social-media-->
						{{> social-media}}
						<!--close social-media-->
					</section>
				</div>
			</div>
		</div>
	</div>
